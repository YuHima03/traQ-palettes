{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
      "System.Net.Http.HttpClient": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Session": {
    "Cookie": {
      "HttpOnly": true,
      "IsEssential": true
    },
    "IdleTimeout": "00:05:00"
  },
  "Authentication": {
    "Cookie": {
      "ExpireTimeSpan": "24:00:00"
    }
  },
  "Cache": {
    "Memory": {
      "ExpirationScanFrequency": "00:15:00"
    }
  }
}
